{
  "name": "masked-calling-backend",
  "version": "1.0.0",
  "description": "Backend API for Masked Calling Parking Alert System",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:watch": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "verify-schema": "ts-node src/scripts/verify-schema.ts",
    "migrate": "ts-node src/scripts/run-migrations.ts",
    "validate-users": "ts-node src/scripts/validate-users-table.ts",
    "validate-vehicles": "ts-node src/scripts/validate-vehicles-table.ts",
    "validate-call-logs": "ts-node src/scripts/validate-call-logs-table.ts",
    "validate-alert-logs": "ts-node src/scripts/validate-alert-logs-table.ts",
    "validate-scan-logs": "ts-node src/scripts/validate-scan-logs-table.ts",
    "test-external-services": "ts-node src/scripts/test-external-services.ts",
    "optimize-logs": "ts-node src/scripts/apply-log-optimizations.ts",
    "log-maintenance": "ts-node src/scripts/log-maintenance.ts",
    "log-stats": "ts-node src/scripts/log-maintenance.ts stats",
    "log-cleanup": "ts-node src/scripts/log-maintenance.ts cleanup",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "masked-calling",
    "parking-alert",
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "qrcode": "^1.5.4",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.0",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "supertest": "^7.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
